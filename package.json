{
  "name": "clean-typescript-vue",
  "version": "1.0.0",
  "author": "Gabriel Scaranello <gabrieljscaranello@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vue-tsc && vite build --emptyOutDir",
    "prepare": "husky install",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.2.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-config-standard": "^5.0.0",
    "sass": "1.32.12",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vue-tsc": "^0.40.13"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.3",
    "quasar": "^2.8.3",
    "vue": "^3.2.39"
  },
  "engines": {
    "node": ">=16.14.0",
    "yarn": ">=1.22.17"
  }
}
